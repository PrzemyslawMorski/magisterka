{
 "execution": [
  {
   "concurrency": 1000,
   "ramp-up": "1m",
   "hold-for": "5m",
   "scenario": "quick-test"
  }
 ],
 "scenarios": {
  "quick-test": {
   "requests": [
    "http://localhost:8080"
   ]
  }
 },
 "reporting": [
  {
   "module": "console"
  },
  {
   "module": "final-stats",
   "summary": true,
   "percentiles": true,
   "summary-labels": true,
   "failed-labels": true,
   "test-duration": true,
   "dump-xml": "results.xml",
   "dump-csv": "results.csv"
  }
 ]
}
